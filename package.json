{
  "name": "notion-blog",
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "prestart": "yarn run build:tailwind",
    "start": "next start",
    "prebuild": "yarn run build:tailwind",
    "build:tailwind": "tailwindcss build src/styles/tailwind.css -o src/styles/tailwind.output.css",
    "build": "next build && node .next/serverless/scripts/build-rss.js",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss,sass}\" --ignore-path .gitignore"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@zeit/react-jsx-parser": "^2.0.0",
    "async-sema": "^3.1.0",
    "autoprefixer": "^10.1.0",
    "github-slugger": "^1.2.1",
    "katex": "^0.12.0",
    "next": "9.4.4",
    "postcss": "^8.2.1",
    "prismjs": "^1.17.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "tailwindcss": "^2.0.2",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@types/katex": "^0.11.0",
    "@types/node": "^13.1.2",
    "@types/react": "^16.9.17",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.4"
  }
}
